{
  "$jsonSchema": {
    "bsonType": "object",
    "required": [
      "user_id",
      "last_status",
      "version"
    ],
    "properties": {
      "user_id": {
        "bsonType": "int",
        "minimum": 0,
        "description": "must be `int` & is required"
      },
      "last_status": {
        "bsonType": "string",
        "enum": ["succeed", "failed"],
        "description": "must be `string` & is required"
      },
      "time_until_generations_successful": {
        "bsonType": "date",
        "description": "must be `date`"
      },
      "version": {
        "bsonType": "string",
        "description": "must be `string` & is required"
      }
    }
  }
}
